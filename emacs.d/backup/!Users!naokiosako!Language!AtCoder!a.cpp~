#include<bits/stdc++.h>
using namespace std;

int main(){
    int N, M, solve=0, pena=0;
    cin >> N >> M;
    vector<int> WA(N+1, 0), AC(N+1, -1);
    vector<int> a(M);
    vector<string> b(M);
    for(int i=0; i<M; i++){
	cin >> a[i] >>b[i];
    }

    for(int i=0; i<M; i++){
	if(b[i]=="WA")
	    WA[a[i]]++;
	else{
	    if(AC[a[i]]==-1){
		AC[a[i]]=0;
		solve++;
		pena += WA[a[i]];
	    }
	}
    }
    
    cout << solve <<" " <<pena<< endl;
}

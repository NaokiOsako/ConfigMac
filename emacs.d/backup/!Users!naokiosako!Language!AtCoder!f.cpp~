#include<bits/stdc++.h>
using namespace std;

vector<vector<int>> G(110);

void dfs(int v, int p=-1) {
    for(auto i: G[v]){
	if(i == p)
	    continue;
	cout << i << endl;
        dfs(i, v);
    }
}

void bfs() {
    queue<int> q;
    q.push(1);

    while(!q.empty()){
	int node = q.front();
	q.pop();
	for(auto i: G[node]){
	    q.push(i);
	    cout << i << endl;
	}

    }
}



int main(){
    int N, a, b;
    cin >> N ;
    for(int i=0; i<N-1; i++){
	cin >> a >> b;
        G[a].push_back(b);
	// G[b].push_back(a);
    }
    // dfs(1);
    bfs();

    cout << endl;
}


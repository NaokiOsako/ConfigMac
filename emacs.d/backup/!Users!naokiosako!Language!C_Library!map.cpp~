#include<bits/stdc++.h>
using namespace std;

int main(){
    int N, ans=0;
    cin >> N;
    map<string, int> mp;
    string s;
    for(int i=0; i<N; i++){
	cin >> s;
	mp[s]++;
    }

    for(auto itr = mp.begin(); itr != mp.end(); ++itr) {
        cout << "key = " << itr->first           // キーを表示
	<< ", val = " << itr->second << "\n";    // 値を表示
    }

    cout << ans << endl;
}

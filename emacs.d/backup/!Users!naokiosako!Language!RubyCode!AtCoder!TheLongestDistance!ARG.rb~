# coding: utf-8
def dis(px1, py1, px2, py2)  
  return ((px1 -px2) ** 2 + (py1 - py2) ** 2) ** (1/2.0)
end

def main(argv)
  f = open(argv[0])
  max =0
  h = f.gets.to_i
  px = []
  py = []
  h.times do
    line = f.gets.chop.split.map(&:to_i)
    px << line.shift
    py << line.shift
  end
  f.close
  
  (0 .. h-1). each do |i|
    (i .. h-1). each do |j|
      a=dis(px[i], py[i], px[j], py[j])
      if a > max
        max =a
      end
    end
  end

  puts max
end

main(ARGV)

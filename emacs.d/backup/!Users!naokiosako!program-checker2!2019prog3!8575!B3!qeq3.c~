#include <stdio.h>
#include <math.h>
#include <float.h>
#include <assert.h>
#include "qeq3.h"

int qeq(double a, double b, double c, double *x1, double *x2) 
{
	double d,result,y1,y2,y0;
	
	d=b*b-4*a*c;
	
	result=sqrt(d);
	*x1=(-b+result)/(2*a);
	*x2=(-b-result)/(2*a);
	
	if(d>0)
	{
		y2=2;
		return y2;
	}
	else if(d==0)
	{
		y1=1;
		return y1;
	}
	else
	{
		return y0;
	}
}



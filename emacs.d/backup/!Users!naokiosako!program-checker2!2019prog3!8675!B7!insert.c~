#include<stdio.h>

int a, b;

void insert(int n, double *a, int k){
	int i;
	
	for (i=k; 0<i; i--) {//a[i-1]≤a[i] なら終了そうでなければ a[i] と a[i-1] の値を交換;
		if(a[i] >= a[i-1]){
		break;
		}
		a[i] += a[i-1];
		a[i-1] = a[i] - a[i-1];
		a[i] -= a[i-1];
	}

}
/*

#include<stdio.h>

int a, b;
#define nsfw(a, b)(a+= b, b= a-b, a -= b)


void insert(int n, double *a, int k) {
	int i;
	
	for (i=k; 0<i; i--) {//a[i-1]≤a[i] なら終了そうでなければ a[i] と a[i-1] の値を交換;
		if(a[i] >= a[i-1]){
			nsfw(a[i], a[i-1]);
		}
		break;
	}
	
}


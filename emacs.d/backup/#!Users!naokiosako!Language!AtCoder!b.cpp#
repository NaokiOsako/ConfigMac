#include<bits/stdc++.h>
using namespace std;

int main() {
    string S;
    cin >> S;
    int siz = S.size();
    bool flag = true;

    for(int i=0; i<S.size()/2; i++){
	if(S[i] != S[siz-1-i])
	    flag=false;
    }

    for(int i=0; i<(siz-1)/2/2; i++){
	if(S[i] != S[(siz-1)/2-1-i])
	    flag=false;
    }

    for(int i=(siz+3)/2-1; i<S.size(); i++){
	if(S[i] != S[siz-1-i])
	    flag=false;
    }

    cout << (flag? "Yes": "No") << endl;

} 


